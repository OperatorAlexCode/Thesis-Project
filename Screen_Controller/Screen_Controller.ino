void SetTile(int tileToSet, int room) {
  Multi1.openChannel(tileToSet%8);

  Screen1.clearBuffer();

  switch(tileToSet){
    case 0:
    Screen1.drawXBMP(0,0,128,128,Bunks);
    break;
    case 1:
    Screen1.drawXBMP(0,0,128,128,AiCore);
    break;
    default:
    String output = String("Screen ") + String(tileToSet);
    Screen1.drawStr(0, 10, output.c_str());
    break;
  }

  Screen1.sendBuffer();

  Multi1.closeChannel(tileToSet%8);
}